# It's not recommended to modify this unit file because your changes
# would be overwritten during the package update.

[Unit]
Description=GECOS Help Channel web socket client
Wants=basic.target
After=basic.target network.target 

[Service]
Type=forking
ExecStart=/usr/bin/websockify -D localhost:6900 localhost:5900 >> /var/log/gecos/gecoshc_ws_client.log 2>&1
PIDFile=/var/run/gecoshc_ws_client.pid

[Install]
WantedBy=multi-user.target

