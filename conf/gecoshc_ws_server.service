# It's not recommended to modify this unit file because your changes
# would be overwritten during the package update.

[Unit]
Description=GECOS Help Channel web socket server
Wants=basic.target
After=basic.target network.target 

[Service]
Type=forking
ExecStart=/usr/bin/websockify -D localhost:6500 localhost:5500 >> /var/log/gecos/gecoshc_ws_server.log 2>&1
PIDFile=/var/run/gecoshc_ws_server.pid

[Install]
WantedBy=multi-user.target

